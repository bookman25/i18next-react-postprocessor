    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "i18next": "^8.4.3",
    "jest": "^20.0.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "regenerator-runtime": "^0.11.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "peerDependencies": {
    "react": "^15.6.1"
  },
